{
  "name": "accounts-prewebpack",
  "packageManager": "yarn@3.1.1",
  "scripts": {
    "build:tsc": "tsc -b",
    "build:rollup": "rollup --config rollup.config.js",
    "build": "yarn run build:tsc && yarn run build:rollup",
    "dev:tsc": "tsc -b -w",
    "dev:rollup": "rollup --config rollup.config.js --watch",
    "dev": "concurrently \"yarn run dev:tsc\" \"sleep 5 && yarn run dev:rollup\"",
    "test": "live-server --port=8080 --host=localhost --watch=\"dist/index.mjs,dist/*.html,dist/*.css\" --open=index.html ./dist"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@types/linkifyjs": "^2.1.4",
    "console-feed": "^3.3.0",
    "debug": "^4.3.3",
    "mobx": "^6.4.2",
    "mobx-react-lite": "^3.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup-plugin-copy-watch": "^0.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/debug": "^4",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17",
    "concurrently": "^7.0.0",
    "live-server": "^1.2.1",
    "rollup": "^2.68.0",
    "typescript": "^4.5.5"
  }
}
