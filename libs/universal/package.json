{
  "name": "@aultfarms/universal",
  "version": "0.0.3",
  "description": "Browser-based Libraries for Ault Farms data models",
  "type": "module",
  "comment1": "dist/index.js is normal output of tsc, used for node",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "comment2": "dist/browser.mjs took index.js and rolled it up w/ rollup",
  "browser": "dist/browser.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:tsc": "tsc -b",
    "build:rollup": "rollup --config=rollup.config.js",
    "build": "yarn build:tsc && yarn build:rollup",
    "dev:tsc": "tsc -b -w",
    "dev:rollup": "rollup --config rollup.config.js --watch",
    "dev": "concurrently \"yarn run dev:tsc\" \"yarn run dev:rollup\"",
    "test:browser": "live-server --port=8080 --host=localhost --wait=1000 --open=test/browser.html",
    "test:node": "nodemon --enable-source-maps --config nodemon.config.json dist/test/node.js",
    "test": "concurrently \"yarn test:browser\" \"yarn test:node\""
  },
  "author": "Aaron Ault <aultac@gmail.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@tsconfig/node16": "^1.0.2",
    "@types/bluebird": "^3",
    "@types/debug": "^4",
    "@types/gapi": "^0.0.41",
    "@types/gapi.auth2": "^0.0.56",
    "@types/gapi.client": "^1.0.5",
    "@types/gapi.client.drive": "^3.0.13",
    "@types/gapi.client.sheets": "^4.0.20201029",
    "@types/moment": "^2.13.0",
    "@types/node": "^17.0.14",
    "@types/numeral": "^2",
    "@types/rfdc": "^1",
    "@yarnpkg/sdks": "^2.6.0-rc.4",
    "concurrently": "^7.0.0",
    "deep-equal": "^2.0.5",
    "http-server": "^14.1.0",
    "live-server": "^1.2.1",
    "rollup": "^2.66.1",
    "rollup-plugin-pnp-resolve": "^2.0.0",
    "terser": "^5.10.0",
    "testcafe": "^1.18.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@overleaf/o-error": "^3.4.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/deep-equal": "^1.0.1",
    "@types/omit": "^1.0.0",
    "bluebird": "^3.7.2",
    "chalk": "^5.0.0",
    "debug": "^4.3.3",
    "moment": "^2.29.1",
    "moment-range": "^4.0.2",
    "numeral": "^2.0.6",
    "omit": "^1.0.1",
    "rfdc": "^1.3.0",
    "tslib": "^2.3.1",
    "xlsx": "^0.18.0"
  }
}
