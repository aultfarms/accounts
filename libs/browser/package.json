{
  "name": "@aultfarms/browser",
  "version": "0.0.3",
  "description": "Browser-based Libraries for Ault Farms data models",
  "comment": "Rollup Setup is from https://gist.github.com/danieldietrich/999abe1aaee11dcdf91d182807f7ee3f",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:tsc": "tsc -b",
    "build:rollup": "rollup -c rollup.config.js",
    "build:rollupold": "rollup --config rollup.config.ts --configPlugin typescript",
    "build": "yarn run build:tsc && yarn run build:rollup",
    "dev:tsc": "tsc -w -b",
    "dev:rollup": "rollup --config rollup.config.ts --configPlugin typescript --watch",
    "dev:testcafe": "testcafe --live --config-file=.testcafe.js chrome test/",
    "dev": "concurrently \"yarn run dev:tsc\" \"yarn run dev:rollup\" \"yarn run dev:testcafe\"",
    "test": "testcafe --config-file='.testcafe.js' chrome test/"
  },
  "author": "Aaron Ault <aultac@gmail.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@tsconfig/node16": "^1.0.2",
    "@types/bluebird": "^3",
    "@types/debug": "^4",
    "@types/gapi": "^0.0.41",
    "@types/gapi.auth2": "^0.0.56",
    "@types/gapi.client": "^1.0.5",
    "@types/gapi.client.drive": "^3.0.13",
    "@types/gapi.client.sheets": "^4.0.20201029",
    "@types/moment": "^2.13.0",
    "@yarnpkg/sdks": "^2.6.0-rc.4",
    "concurrently": "^7.0.0",
    "rollup": "^2.66.1",
    "rollup-plugin-pnp-resolve": "^2.0.0",
    "terser": "^5.10.0",
    "testcafe": "^1.18.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@googleapis/drive": "^2.1.0",
    "@googleapis/sheets": "^0.3.0",
    "@overleaf/o-error": "^3.4.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "bluebird": "^3.7.2",
    "debug": "^4.3.3",
    "moment": "^2.29.1",
    "tslib": "^2.3.1"
  }
}
