{
  "name": "@aultfarms/browser",
  "version": "0.0.3",
  "description": "Browser-based Libraries for Ault Farms data models",
  "main": "dist/index.mjs",
  "type": "module",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:tsc": "tsc -b",
    "build:webpack": "webpack --config=webpack.config.cjs",
    "build:rollup": "rollup --config=rollup.config.js",
    "build": "yarn build:tsc && yarn build:rollup",
    "dev:tsc": "tsc -b -w",
    "dev:rollup": "rollup --config rollup.config.js --watch",
    "dev:testcafe": "testcafe --live --config-file=.testcafe.js",
    "dev": "concurrently \"yarn run dev:tsc\" \"yarn run dev:rollup\"",
    "test:testcafe": "testcafe --config-file='.testcafe.js'",
    "test": "live-server --port=8080 --host=localhost --wait=1000 --open=test/index.html"
  },
  "author": "Aaron Ault <aultac@gmail.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@tsconfig/node16": "^1.0.2",
    "@types/bluebird": "^3",
    "@types/debug": "^4",
    "@types/gapi": "^0.0.41",
    "@types/gapi.auth2": "^0.0.56",
    "@types/gapi.client": "^1.0.5",
    "@types/gapi.client.drive": "^3.0.13",
    "@types/gapi.client.sheets": "^4.0.20201029",
    "@types/moment": "^2.13.0",
    "@types/node": "^17.0.14",
    "@yarnpkg/sdks": "^2.6.0-rc.4",
    "concurrently": "^7.0.0",
    "deep-equal": "^2.0.5",
    "http-server": "^14.1.0",
    "live-server": "^1.2.1",
    "rollup": "^2.66.1",
    "rollup-plugin-pnp-resolve": "^2.0.0",
    "terser": "^5.10.0",
    "testcafe": "^1.18.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@googleapis/drive": "^2.1.0",
    "@googleapis/sheets": "^0.3.0",
    "@overleaf/o-error": "^3.4.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/deep-equal": "^1.0.1",
    "bluebird": "^3.7.2",
    "debug": "^4.3.3",
    "moment": "^2.29.1",
    "tslib": "^2.3.1"
  }
}
