{
  "name": "@aultfarms/google",
  "version": "1.0.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "description": "Browser-based library for interacting with google drive and sheets",
  "main": "dist/index.mjs",
  "type": "module",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:tsc": "tsc -b",
    "build:rollup": "rollup --config=rollup.config.js",
    "build": "yarn build:tsc && yarn build:rollup",
    "dev:tsc": "tsc -b -w",
    "dev:rollup": "rollup --config rollup.config.js --watch",
    "dev:testcafe": "testcafe --live --config-file=.testcafe.js",
    "dev": "yarn run build:tsc && concurrently \"yarn run dev:tsc\" \"yarn run dev:rollup\"",
    "test:testcafe": "testcafe --config-file='.testcafe.js'",
    "test": "live-server --port=8080 --host=localhost --wait=1000 --watch=dist/index.mjs,dist/test/index.mjs --open=test/index.html",
    "clean": "rm -rf dist"
  },
  "author": "Aaron Ault <aultac@gmail.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@types/google.accounts": "^0.0.14",
    "@yarnpkg/sdks": "^3.0.0-rc.42",
    "concurrently": "^8.0.1",
    "deep-equal": "^2.2.0",
    "http-server": "^14.1.1",
    "live-server": "^1.2.2",
    "rollup": "^3.20.2",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-pnp-resolve": "^2.0.0",
    "terser": "^5.16.8",
    "testcafe": "^1.18.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "@googleapis/drive": "^2.1.0",
    "@googleapis/sheets": "^0.3.0",
    "@overleaf/o-error": "^3.4.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@tsconfig/node16": "^1.0.3",
    "@types/bluebird": "^3.5.38",
    "@types/debug": "^4.1.7",
    "@types/deep-equal": "^1.0.1",
    "@types/gapi": "^0.0.47",
    "@types/gapi.client": "^1.0.8",
    "@types/gapi.client.drive": "^3.0.15",
    "@types/gapi.client.sheets-v4": "^0.0.4",
    "@types/moment": "^2.13.0",
    "@types/node": "^17.0.14",
    "bluebird": "^3.7.2",
    "debug": "^4.3.4",
    "moment": "^2.29.4",
    "p-limit": "^4.0.0",
    "p-reduce": "^3.0.0",
    "q-i": "^2.0.1",
    "tslib": "^2.5.0",
    "xlsx-js-style": "^1.2.0"
  }
}
